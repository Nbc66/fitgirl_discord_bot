    "version": "0.2.0",
    "configurations": [
        {
            "name": "bot Debugger: Launch with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bot.py",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        },
        {
            "name": "bot Debugger: Debug on test Guild",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bot.py",
            "console": "integratedTerminal",
            "args": [
                "--test"
            ]
        }
    ]
}